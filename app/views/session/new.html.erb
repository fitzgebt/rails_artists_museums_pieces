<%= form_tag('/session', method: :post) do %>
    <%= label_tag(:username, "Your username:") %>
    <%= collection_select(:artist, :username, Artist.all, :username, :username, prompt: "Please select your username from this dropdown") %>
    <%= label_tag(:password, "Your password:") %>
    <%= password_field_tag(:password, nil, class: "form-control") %>
 
    <%= submit_tag("Sign In") %>
<% end %> 